<div class="row">
    <div class="col-xs-6 col-md-6">
        <%= form.text_field :first_name, class: "form-control", placeholder: "First Name", required: true, autofocus: true%>
    </div>
    <div class="col-xs-6 col-md-6">
        <%= form.text_field :last_name, class: "form-control", placeholder: "Last Name", required: true, autofocus: true%>
    </div>
</div>

<%= form.text_field :email, type: 'email', class: 'form-control', placeholder: 'Your Email' %>
<%= form.password_field :password, class: 'form-control', placeholder: 'Your Password' %>
<%= password_field_tag :password_confirmation, nil, class: "form-control", placeholder: 'Re-Enter Password' %>

<div class="form-group">
  <%= form.label :birthday %>
  <%= form.date_select :birthday %>
</div>
<div class="form-group">
  <%= form.label :gender %>
  <%= form.select :gender do %>
      <option value="1">Male</option>
      <option value="2">Female</option>
  <% end %>
</div>
