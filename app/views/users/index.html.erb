
<div class="container-fluid" style="background-color:#e8e8e8">
  <div class="container container-pad" id="property-listings">

      <div class="row">
          <div class="col-sm-12">
            <% @users_per_page.each do |user| %>
              <!-- Begin Listing: 609 W GRAVERS LN-->
              <div class="brdr bgc-fff pad-10 box-shad btm-mrg-20 property-listing">
                  <div class="media">
                      <a class="pull-left" href="#" target="_parent">
                      <%= image_tag 'default-picture-property.jpg', :class=> 'img-responsive', :alt=> 'image'%>
                      </a>

                      <div class="clearfix visible-sm"></div>

                      <div class="media-body fnt-smaller">
                          <a href="#" target="_parent"></a>

                          <h4 class="media-heading">
                            <a href="/users/<%=user.id%>" target="_parent"><%= user.first_name %>,<%=user.last_name%></a>
                          </h4>

                          <p> <%=user.email %> </p>


                          <p class="hidden-xs"><%=user.role%></p><span class="fnt-smaller fnt-lighter fnt-arial"></span>
                      </div>
                  </div>
              </div><!-- End Listing-->
            <%end%>
            <%if @users_per_page.first.id == @users.first.id %>
                <div class='col-sm-1'>
                </div>
                <div class='col-sm-10'>
                </div>
                <div class='col-sm-1'>
                  <%=form_tag users_path, method: :get do %>
                    <%= hidden_field_tag "value", 10 %>
                    <%= submit_tag ">" %>
                  <% end%>
                </div>
            <%elsif @users_per_page.last.id == @users.last.id%>
              <%=form_tag users_path, method: :get do %>
                <%= hidden_field_tag "value",  @counter - 10 %>
                <%= submit_tag "<" %>
              <% end%>
            <%else%>
              <div class='col-sm-1'>
                <%=form_tag users_path, method: :get do %>
                  <%= hidden_field_tag "value",  @counter - 10 %>
                  <%= submit_tag "<" %>
                <% end%>
              </div>
              <div class='col-sm-10'>
              </div>
              <div class='col-sm-1'>
                <%=form_tag users_path, method: :get do %>
                  <%= hidden_field_tag "value",  @counter + 10 %>
                  <%= submit_tag ">" %>
                <% end%>
              </div>
            <%end%>
          </div><!-- End Col -->
      </div><!-- End row -->
  </div><!-- End container -->
</div>
